<div class="container">
    <div class="w-100 p-1 container d-flex about-section row">
        <h1 class="w-100 p-1 d-flex justify-content-center about-header1">About the Team</h1>
    </div>
    <div class="row">
        <p class="d-flex row w-100 p-3" id="about-text">Some text that needs filled in later</p>
    <div class="row">
    <div class="w-100 p-1 container d-flex about-section row">
        <h1 class="w-100 p-1 d-flex justify-content-center about-header1">Attributions</h1>
            <ul>
                <li>
                    Expense icon made by <a href="" title="surang">surang</a> from <a href="https://www.flaticon.com/" title="Flaticon">www.flaticon.com</a>
                </li>
                <li>
                    Calculator icon made by <a href="https://www.freepik.com" title="Freepik">Freepik</a> from <a href="https://www.flaticon.com/" title="Flaticon">www.flaticon.com</a>
                </li>
            </ul>
    </div>

    <div class="container">
        <h3 class="d-flex justify-content-center about-h2">Our Team</h2> <!--should be centered?-->
        <div class="d-flex justify-content-center row">
            {{#each cardList}}
            <div class="col-sm-4">
                <div class="card mt-2 mb-2">
                    <img class="card-img-top img-portrait p-4" src="{{this.imgsrc}}">
                    <div class="card-body">
                        <h4 class="card-title lead">{{this.contributor}}</h4>
                        <h6 class="card-subtitle mb-2 text-muted">{{this.title}}</h6>
                        <p class="card-text">{{this.abouttext}}</p>
                        <div class="d-flex row justify-content-around">
                            <div class="btn btn-dark d-flex justify-content-center col-sm-2">
                                <a href={{this.github}}><img alt="GitHub Mark" src="/images/gh-mark.png"></a>
                            </div>
                            <div class="btn btn-dark d-flex justify-content-center col-sm-2">
                                <a href="mailto:{{this.email}}?subject=Re: Capital Opulence"><img alt="Email Mark" src="/images/email-mark.png"></a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            {{/each}}
        </div>
    </div>
</div>